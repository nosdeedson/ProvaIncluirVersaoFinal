{
    "name": "marciodojr/provaincluir2019",
    "description": "Prova para desenvolvimento de api's",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "license": "MIT",
    "authors": [],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "doctrine/migrations": "^2.1",
        "doctrine/orm": "^2.6",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^1.24",
        "php-di/slim-bridge": "^3.0",
        "slim/psr7": "^0.5",
        "slim/slim": "^4.1"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.3",
        "phpstan/phpstan": "^0.11.16",
        "phpstan/phpstan-doctrine": "^0.11.6",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
